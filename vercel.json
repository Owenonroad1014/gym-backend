{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/img/(.*)",
        "dest": "/img/$1"
      },
      {
        "src": "/img/avatar/(.*)",
        "dest": "/img/avatar/$1"
      },
      {
        "src": "/imgs/(.*)",
        "dest": "/imgs/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  